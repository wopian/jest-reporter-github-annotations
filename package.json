{
  "name": "@wopian/jest-reporter-github-annotations",
  "version": "0.0.2",
  "description": "Report Jest test failures with GitHub annotations",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint ./ --fix",
    "lint:ci": "eslint ./",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --silent",
    "coverage": "NODE_OPTIONS=--experimental-vm-modules jest --silent --coverage",
    "coverage:ci": "NODE_OPTIONS=--experimental-vm-modules jest --silent --coverage --maxWorkers=2 --reporters=default --reporters=./src/index.js"
  },
  "engines": {
    "node": ">= 14 < 15 || >= 16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wopian/jest-reporter-github-annotations.git"
  },
  "author": "wopian <wopian@wopian.me> (https://wopian.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wopian/jest-reporter-github-annotations/issues"
  },
  "homepage": "https://github.com/wopian/jest-reporter-github-annotations#readme",
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "jest",
    "reporter",
    "reporters",
    "integration",
    "github",
    "annotations",
    "ci",
    "cd",
    "actions",
    "workflow",
    "results",
    "processor"
  ],
  "renovate": {
    "extends": [
      "github>wopian/renovate-config"
    ]
  }
}
